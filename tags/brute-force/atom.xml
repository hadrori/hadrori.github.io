</pre></td><td class='code'><pre><code class='cpp'><span class='line'><span class="cp">#include &lt;bits/stdc++.h&gt;</span>
</span><span class='line'><span class="k">using</span> <span class="k">namespace</span> <span class="n">std</span><span class="p">;</span>
</span><span class='line'><span class="cp">#define rep(i,a) for(int i=0;i&lt;(int)(a);i++)</span>
</span><span class='line'><span class="cp">#define repit(i,a) for(auto i=begin(a);i!=end(a);i++)</span>
</span><span class='line'><span class="kt">int</span> <span class="n">G</span><span class="p">[</span><span class="mi">128</span><span class="p">][</span><span class="mi">128</span><span class="p">],</span> <span class="n">N</span><span class="p">,</span> <span class="n">M</span><span class="p">,</span> <span class="n">fr</span><span class="p">[</span><span class="mi">128</span><span class="p">];</span>
